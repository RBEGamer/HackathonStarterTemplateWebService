<body class="container">

    <nav class="navbar navbar-light bg-light static-top">
        <div class="container">
            <a class="navbar-brand" href="/"><%=app_name  %></a>
           
        </div>
    </nav>

    <!-- Masthead -->
    <form action="/register" method="POST">
        <header class="masthead text-white text-center">
            <div class="overlay"></div>
            <div class="container">
                <div class="row">
                    <div class="col-xl-9 mx-auto">
                        <h1 class="mb-5">REGISTER</h1>
                    </div>
                    <div class="col-md-10 col-lg-8 col-xl-7 mx-auto">
                        <input type="text" id="name" name="name" value="" placeholder="NAME" /><br><br>
                        <input type="email" id="email" name="email" value="" placeholder="YOUR@NAME.TLD" /><br><br>
                        <input type="password" id="password" name="password" value="" placeholder="*****" /><br><br>

                        <input type="submit" class="btn btn-block btn-danger" value="REGISTER" />

                    </div>

                    <noscript>
                        <div class="alert alert-danger">
                            <strong>Fehler :</strong> Bitte aktivieren Sie Javascript um alle Features sehen zu koennen.
                        </div>
                    </noscript>



                </div>
            </div>
            </div>
        </header>
    </form>








    <!-- Footer -->
    <footer class="footer bg-light">
        <div class="container text-center">
            <div class="row">

                <div class="col-lg-4 h-100 text-center text-lg-center my-auto">
                    <a href="/"><img src="img/sponsor_a.png" height="100px"></a>
                </div>

                <div class="col-lg-4 h-100 text-center text-lg-center my-auto">
                    <a href="/"><img src="img/sponsor_b.png" height="100px"></a>
                </div>

                <div class="col-lg-4 h-100 text-center text-lg-center my-auto">
                    <a href="/"><img src="img/sponsor_c.png" height="100px"></a>
                </div>
            </div>

            
        </div>



    </footer>






